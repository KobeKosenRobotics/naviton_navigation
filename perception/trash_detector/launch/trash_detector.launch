<launch>

    <arg name="param_file" default="$(find trash_detector)/config/trash_detector.yaml"/>
    
    <group ns="trash_detector">
        
        <!-- Trash Detector -->
        <node pkg="trash_detector" type="trash_detector_node" name="trash_detector_node" output="screen">
            <rosparam command="load" file="$(arg param_file)" />
        </node>

    </group>

</launch>